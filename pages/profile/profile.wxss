.avatar {
  height: 300rpx;
  background-color: #584f60;
  display: flex;
  justify-content: start;
  align-items: center;
}
.avatar image {
  width: 200rpx;
  height: 200rpx;
  margin: 0 30rpx;
  border-radius: 50%;
}
.avatar .uname{
  font-size: 24px;
  color: #fff;
}
/*用户数据列表*/
.userdata{
  display: flex;
  justify-content: start;
}
.userdata .data-item{
  display: flex;
  width: 50%;
  flex-direction: column;
  justify-content: space-between;
  padding: 10rpx;
}
.userdata .data-item:last-child{
  align-items: center;
  justify-content:start;
  padding-top: 30rpx;
}
.userdata .info{
  font-size: 16px;
  padding: 20rpx 0;
}
.userdata .info+text{
  font-size: 12px;
  color:#939393;
  padding: 20rpx 0;
}
.userdata .info+text+text{
  font-size: 36px;
  color: #584f60;
  padding: 10rpx 0;
  padding-bottom: 40rpx;
}
.userdata .data-fans{
  padding: 30rpx 0;
  color: #584f60;
  font-size: 26px;
}
.userdata .data-fans+text{
  font-size: 16px;
}
/*keep列表*/
.container {
  padding: 16rpx;
  display: flex;
  padding-bottom: 0;
}
.keep-box{
  background-color: #fff;
  border-radius: 10rpx;
  padding: 15rpx;
  font-size: 10px;
  width: 22%;
  box-shadow: 0 5rpx 10rpx rgba(0, 0, 0, 0.1);
}
 .keep-box .keep-time {
  font-size: 16px;
  color: #584f60;
  padding-right: 10rpx;
}
/*列表*/
.cell {
  background-color: #fff;
  font-size: 16px;
}

.cell-item {
  border-bottom: 1rpx solid #ddd;
  padding: 20rpx 30rpx;
}